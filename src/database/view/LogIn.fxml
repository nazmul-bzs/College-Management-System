<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" accessibleRole="COMBO_BOX" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="database.controller.LogInController">
   <children>
      <Label layoutX="111.0" layoutY="104.0" text="User Name" />
      <Label layoutX="115.0" layoutY="163.0" text="Password" />
      <TextField fx:id="UserNameField" layoutX="194.0" layoutY="100.0" promptText="User name" />
      <PasswordField fx:id="PasswordField" layoutX="194.0" layoutY="159.0" promptText="Password" />
      <ButtonBar layoutX="157.0" layoutY="251.0" prefHeight="40.0" prefWidth="200.0">
         <buttons>
          <Button fx:id="ResetButton" mnemonicParsing="false" onAction="#ResetAction" text="Reset" />
            <Button fx:id="LoginButton" mnemonicParsing="false" onAction="#LoginAction" text="Login" />
         </buttons>
      </ButtonBar>
      <ComboBox fx:id="ChoiceUser" layoutX="207.0" layoutY="216.0" prefWidth="150.0" promptText="Login As" />
   </children>
</AnchorPane>
